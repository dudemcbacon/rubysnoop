<img style="width:550px" src="img/snoop2.jpg" />
<br></br>
<table class="table  table-hover">
  <thead>
    <tr>
      <th>Address</th>
      <th>Port</th>
      <th>Protocol</th>
      <th>State</th>
      <th>Service</th>
      <th>Title</th>
    </tr>
  </thead>
  <tbody class="table-bordered">
    <% @scan.each_up_host do |host| %>
      <% host.each_open_port.with_index do |port, index| %>
        <tr>
          <% if index == 0 %>
            <td rowspan="<%= host.open_ports.length %>"><%= r host.ip %></td>
          <% end %>
          <td><%= port.number %></td>
          <td><%= port.protocol %></td>
          <td><%= port.state %></td>
          <td><%= port.service %></td>
          <td><%= get_title "http://#{host.ip}:#{port}" %></td>
        </tr>
      <% end %>
    <% end %>
  </tbody>
</table>
